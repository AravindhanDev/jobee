				<h4><%= data.name %></h4>
				<p class="editBtn"><i class="bi bi-pencil-square"></i> Edit</p>
			</div>
			<a href="mailto:<%= data.email %>"><%= data.email %></a>
			<p class="mt-3">
				<a href="tel:<%= data.mobile %>">+(91) <%= data.mobile %></a>
			</p>
			<span style="font-weight: 500; color: #666"><%= data.city %></span>
